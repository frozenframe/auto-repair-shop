<Application x:Class="AutoRepairShop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutoRepairShop"
             xmlns:viewmodels ="clr-namespace:AutoRepairShop.ViewModel"
             xmlns:views ="clr-namespace:AutoRepairShop.View"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:AddWorkViewModel}">
            <views:AddWorkWindow/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:ClientDataViewModel}">
            <views:ClientDataUserControl/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:ClientsViewViewModel}">
            <views:ClientsViewUserControl/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:OpenTreeViewModel}">
            <views:WorkTypeViewUserControl/>
        </DataTemplate>

    </Application.Resources>
</Application>
